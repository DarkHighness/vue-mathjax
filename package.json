    "@types/throttle-debounce": "^2.1.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vite-plugin-dts": "^0.9.9",
    "vue-tsc": "^0.29.8"
  },
  "license": "MIT",
  "keywords": [
    "mathjax",
    "vue"
